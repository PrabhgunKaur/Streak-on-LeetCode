int search(int* nums, int numsSize, int target) {
    for(int i=0; i<numsSize; i++){
        int mid = (numsSize-1)/2;
        if(nums[mid]==target){
            return mid;
        }
        else if(nums[mid]<target){
            for(int i=mid+1; i<numsSize; i++){
                if(nums[i]==target){
                    return i;
                }
            }
            return -1;
        }
        else if(nums[mid]>target){
            for(int i=0; i<mid; i++){
                if(nums[i]==target){
                    return i;
                }
            }
            return -1;
        }
        return -1;
    }
    return -1;
}
