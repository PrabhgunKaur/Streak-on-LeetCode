class Solution {
public:
    char nextGreatestLetter(vector<char>& letters, char target) {
        int n = letters.size();
        int s=0;
        int e = n-1;
        int result = letters[0];
        while(s<=e){
            int mid = e + (s-e)/2;
            if(letters[mid]>target){
                result = letters[mid];
                e= mid-1;
            }
            else s= mid+1;
        }
        return result;
    }
};
